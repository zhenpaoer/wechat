.comp-select-city {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    background: #fff;
    z-index: 1;
}

.comp-select-city .header-img {
    width: 32rpx;
    height: 32rpx;
}

.comp-select-city .header-text {
    margin-left: 20rpx;
}

.comp-select-city .header-city-box {
    position: absolute;
    left: 30rpx;
    top: 0;
    height: 100%;
}

.comp-select-city .header-city {
    position: relative;
    padding-right: 30rpx;
}

.comp-select-city .header-city:after {
    position: absolute;
    content: "";
    right: 0;
    top: 38rpx;
    width: 0;
    height: 0;
    border-top: 10rpx solid #000;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
}